        "kisela kiša"@hr,
        "savas eső"@hu,
        "pioggia acida"@it,
        "酸性雨"@ja,
        "rūgštusis lietus"@lt,
        "skābais lietus"@lv,
        "zure regen"@nl,
        "Sur nedbør"@no,
        "sur nedbør"@no,
        "Kwaśne opady"@pl,
        "kwaśny deszcz"@pl,
        "chuva ácida"@pt,
