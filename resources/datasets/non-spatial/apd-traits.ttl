    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "cushion-forming"@en .

:stem_growth_habit_decumbent
    a skos:Concept ;
    skos:broader :trait_0030023 ;
    skos:definition "Stems whose bases lie flat on the ground but whose tips grow upward. (Synonyms, ascending)"@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "decumbent"@en .

:stem_growth_habit_dense
    a skos:Concept ;
    skos:broader :trait_0030023 ;
    skos:definition "Plant with a high density of branches."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "dense"@en .

:stem_growth_habit_erect
    a skos:Concept ;
    skos:broader :trait_0030023 ;
    skos:definition "Stems that grow upright."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "erect"@en .

:stem_growth_habit_floating
    a skos:Concept ;
    skos:broader :trait_0030023 ;
    skos:definition "Plant where most of the leaves are floating on water."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "floating"@en .

:stem_growth_habit_mat-forming
    a skos:Concept ;
    skos:broader :trait_0030023 ;
    skos:definition "Multiple stems that are mostly horizontal and are densely packed but open enough that there are still a few gaps between them."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "mat-forming"@en .

:stem_growth_habit_open
    a skos:Concept ;
    skos:broader :trait_0030023 ;
    skos:definition "Plant with an open architecture of branches."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "open"@en .

:stem_growth_habit_prostrate
    a skos:Concept ;
    skos:broader :trait_0030023 ;
    skos:definition "Stems lie flat on the ground but do not root at nodes. (Synonyms, procumbent, reclining, trailing)"@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "prostrate"@en .

:stem_growth_habit_rhizomatous
    a skos:Concept ;
    skos:broader :trait_0030023 ;
    skos:definition "Plants with short horizontal underground stems from which shoots emerge."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "rhizomatous"@en .

:stem_growth_habit_rosette
    a skos:Concept ;
    skos:broader :trait_0030023 ;
    skos:definition "Plants whose leaves radiate from a central point at or near ground level. (Synonyms, acaulescent, rosulate)"@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "rosette"@en .

:stem_growth_habit_spindly
    a skos:Concept ;
    skos:broader :trait_0030023 ;
    skos:definition "Plants with weak, skinny branches."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "spindly"@en .

:stem_growth_habit_sprawling
    a skos:Concept ;
    skos:broader :trait_0030023 ;
