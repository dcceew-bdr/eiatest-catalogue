    skos:prefLabel "rhomboidal"@en .

:leaf_shape_spathulate
    a skos:Concept ;
    skos:broader :trait_0011311 ;
    skos:definition "A 2-dimensional shape that can be approximately oblong, obovate, or oblanceolate in outline with a long attenuate base, like a spoon. (Synonyms, spoon-shaped, clavate)"@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "spathulate"@en .

:leaf_shape_strap-shaped
    a skos:Concept ;
    skos:broader :trait_0011311 ;
    skos:definition "A 2-dimensional shape that is strap-shaped with a rounded tip and a length:width ratio greater than 12:1."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "strap-shaped"@en .

:leaf_shape_subulate
