    skos:prefLabel "narrowly_rhomboidal"@en .

:leaf_shape_narrowly_triangular
    a skos:Concept ;
    skos:broader :trait_0011311 ;
    skos:definition "A 2-dimensional shape that is narrowly triangular to wedge-shaped, with a length:width ratio 6:1 to 3:1 and the petiole attaching near the widest portion of the leaf. (Synonyms, triangular)"@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "narrowly_triangular"@en .

:leaf_shape_obcordate
    a skos:Concept ;
    skos:broader :trait_0011311 ;
    skos:definition "A 2-dimensional heart shape, approximately ovate with a cordate apex, and the petiole attaching along the broad, curve side."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "obcordate"@en .

:leaf_shape_oblanceolate
    a skos:Concept ;
    skos:broader :trait_0011311 ;
    skos:definition "A 2-dimensional shape that is narrowly teardrop-shaped, with a length:width ratio between 6:1 and 3:1. The leaf lamina has curved outer margins and narrows nearly to a point where it attaches to the petiole."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "oblanceolate"@en .

:leaf_shape_oblate
    a skos:Concept ;
    skos:broader :trait_0011311 ;
    skos:definition "A 2-dimensional shape that is a broad oval, with curved margins, lacking a distinct point, and a length:width ratio of approximately 5:6. The petiole attaches along the broader side."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "oblate"@en .

:leaf_shape_oblong
    a skos:Concept ;
    skos:broader :trait_0011311 ;
    skos:definition "A 2-dimensional shape that is somewhat elongated with parallel sides and a length:width ratio between 2:1 and 3:2."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "oblong"@en .

:leaf_shape_obovate
    a skos:Concept ;
    skos:broader :trait_0011311 ;
    skos:definition "A 2-dimensional shape that is egg-shaped with a length:width ratio between 2:1 and 3:2. The leaf lamina has curved outer margins and narrows nearly to a point where it attaches to the petiole."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "obovate"@en .

:leaf_shape_obtriangular
    a skos:Concept ;
    skos:broader :trait_0011311 ;
    skos:definition "A 2-dimensional shape that is triangular to fan-shaped or wedge-shaped, with a length:width ratio 2:1 to 3:2 and the petiole attaching near the narrowest portion of the leaf. (Synonyms, flabelliform, fan-shaped, wedge-shaped, cuneate)"@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "obtriangular"@en .

:leaf_shape_obtrullate
    a skos:Concept ;
    skos:broader :trait_0011311 ;
    skos:definition "A 2-dimensional shape that is approximately parallelogram-shaped, widest near the tip, and with a length:width ratio 2:1 to 3:2."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "obtrullate"@en .

:leaf_shape_orbicular
    a skos:Concept ;
    skos:broader :trait_0011311 ;
    skos:definition "A 2-dimensional shape that is approximately circular. (Synonyms, circular)"@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "orbicular"@en .

:leaf_shape_ovate
    a skos:Concept ;
    skos:broader :trait_0011311 ;
    skos:definition "A 2-dimensional shape that is egg-shaped, with a length:width ratio between 2:1 and 3:2. The petiole attaches near the wide, curved portion of the leaf lamina."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "ovate"@en .

:leaf_shape_peltate
    a skos:Concept ;
    skos:broader :trait_0011311 ;
    skos:definition "A 2-dimensional orbicular shape, distinguished by the petiole attaching near the center of the leaf lamina."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
