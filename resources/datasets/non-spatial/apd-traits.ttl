    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "adventitious_roots"@en .

:plant_climbing_mechanism_hooks
    a skos:Concept ;
    skos:broader :trait_0030021 ;
    skos:definition "Curved stem outgrowths which allow climbers to cling to a host plant."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "hooks"@en .

:plant_climbing_mechanism_scrambling
    a skos:Concept ;
    skos:broader :trait_0030021 ;
    skos:definition "Stem growth habit, where the plant stems are too weak to support themselves, and instead lean on and are supported by host vegetation. There are no specialised structures employed."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "scrambling"@en .

:plant_climbing_mechanism_spines
    a skos:Concept ;
    skos:broader :trait_0030021 ;
    skos:definition "Straight stem outgrowths (including true spines as well as thorns and prickles) which allow climbers to cling to a host plant."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "spines"@en .

:plant_climbing_mechanism_tendrils
    a skos:Concept ;
    skos:broader :trait_0030021 ;
    skos:definition "Modified leaves, petioles, or specialised axillary shoots that coil tightly, forming a specialised structure for climbing."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "tendrils"@en .

:plant_climbing_mechanism_twining
    a skos:Concept ;
    skos:broader :trait_0030021 ;
    skos:definition "Stem growth habit, where the plant stems twist or coil around a host plant to provide support."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "twining"@en .

:plant_flood_regime_classification_amphibious
    a skos:Concept ;
    skos:broader :trait_0030811 ;
    skos:definition "Species which germinate in damp or flooded conditions and can grow in or our of water."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "amphibious"@en .

:plant_flood_regime_classification_amphibious_fluctuation_responders_floating
    a skos:Concept ;
    skos:broader :trait_0030811 ;
    skos:definition "Species which germinate in flooded conditions, grow in both flooded and damp conditions, reproduce above the surface of the water, and which have floating leaves when inundated. (Species with floating leaves; ARf)"@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "amphibious_fluctuation_responders_floating"@en .

:plant_flood_regime_classification_amphibious_fluctuation_responders_plastic
    a skos:Concept ;
    skos:broader :trait_0030811 ;
    skos:definition "Species which germinate in flooded conditions, grow in both flooded and damp conditions, reproduce above the surface of the water, and which have morphological plasticity (e.g. heterophylly) in response to water-level variation. (Morphologically plastic species; ARp)"@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "amphibious_fluctuation_responders_plastic"@en .

:plant_flood_regime_classification_amphibious_fluctuation_tolerators_emergent
    a skos:Concept ;
    skos:broader :trait_0030811 ;
    skos:definition "Species which germinate in damp or flooded conditions, which tolerate variation in water level, and which grow with their basal portions under water and reproduce out of water. (Emergent species; ATe)"@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "amphibious_fluctuation_tolerators_emergent"@en .

:plant_flood_regime_classification_amphibious_fluctuation_tolerators_low
    a skos:Concept ;
    skos:broader :trait_0030811 ;
    skos:definition "Species which germinate in damp or flooded conditions, which tolerate variation in water level, which are low-growing and tolerate complete submersion when water-levels rise. (Low-growing species; ATl)"@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "amphibious_fluctuation_tolerators_low"@en .

:plant_flood_regime_classification_submerged
    a skos:Concept ;
    skos:broader :trait_0030811 ;
    skos:definition "Species which germinate, grow and reproduce underwater. (S"@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "submerged"@en .

:plant_flood_regime_classification_terrestrial_damp
    a skos:Concept ;
    skos:broader :trait_0030811 ;
    skos:definition "Species which germinate, grow and reproduce on saturated soil. (Damp species; Tda)"@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "terrestrial_damp"@en .

:plant_flood_regime_classification_terrestrial_dry
    a skos:Concept ;
    skos:broader :trait_0030811 ;
    skos:definition "Species which germinate, grow and reproduce where there is no surface water and the water table is below the soil surface. (Dry species; Tdr)"@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "terrestrial_dry"@en .

:plant_growth_form_basal_large
    a skos:Concept ;
    skos:broader :trait_0030010 ;
    skos:definition "A plant whose leaves form a distinctly large basal tuft or rosette, including large acaulescent monocots and cycads."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "basal_large"@en .

:plant_growth_form_climber
    a skos:Concept ;
    skos:broader :trait_0030010 ;
    skos:definition "Plant that climbs up another plant's stem or branches, rather than being able to support itself. Term used only if datasets do not specify if a climbing plant is herbaceous (a vine) or woody (a liana)."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "climber"@en .

:plant_growth_form_climber_herbaceous
    a skos:Concept ;
    skos:broader :trait_0030010 ;
    skos:definition "Herbaceous plant that climbs up another plant's stem or branches, rather than being able to support itself. This is usually synonymous with `vine`. Types of climbers (scrambling, twining) are captured under the trait `plant_climbing_mechanism`. (Synonyms, vine)"@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "climber_herbaceous"@en .

:plant_growth_form_climber_woody
    a skos:Concept ;
    skos:broader :trait_0030010 ;
    skos:definition "Woody or thick-stemmed (>2 cm) plant that climbs up another plant's stem or branches, rather than being able to support itself. This is usually synonymous with `liana`. Woody climbers generally use hooks, tendrils, and/or adventitious roots to climb; the climbing mechanisms used by a taxon are captured under the trait `plant_climbing_mechanism`. (Synonyms, liana)"@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "climber_woody"@en .

:plant_growth_form_fern
    a skos:Concept ;
    skos:broader :trait_0030010 ;
    skos:definition "A taxonomic grouping that includes acaulescent ferns; ferns are vascular plants that reproduce via spores and have neither seeds nor flowers."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "fern"@en .

:plant_growth_form_geophyte
    a skos:Concept ;
    skos:broader :trait_0030010 ;
    skos:definition "Subcategory of herb, specifically referring to plants that survive the winter/dry season as an organ (rhizome, bulb, corm, or tuber) buried in dry soil, with all leaves dying back each year."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "geophyte"@en .

:plant_growth_form_graminoid
    a skos:Concept ;
    skos:broader :trait_0030010 ;
    skos:definition "Herbaceous plant with a grass-like morphology that is within the order Poales. Taxa both with and without a tussock form are mapped to this term. This term is most frequently applied to three large monocot families, Poaceae, Cyperaceae, and Juncaceae, but the term could apply to other taxa within Poales with a grass-like morphology, including narrow, linear leaves and non-showy flowers, including Restionaceae, Centrolepidaceae, Anarthriaceae, and Ecdeiocoleaceae. Taxa outside Poales with a grass-like morphology should be designated as `herbs` and also have the term `tufted` mapped to the trait `stem_growth_habit`."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "graminoid"@en .

:plant_growth_form_graminoid_not_tussock
    a skos:Concept ;
    skos:broader :trait_0030010 ;
    skos:definition "A subcategory of graminoid, indicating a plant which explicitly lacks a tussock morphology, with the leaves not growing in tufts."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "graminoid_not_tussock"@en .

:plant_growth_form_herb
    a skos:Concept ;
    skos:broader :trait_0030010 ;
    skos:definition "A seed-bearing plant which does not have a woody above-ground stem; when available this term has been divided into more detailed terms. (Synonyms, forb)"@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "herb"@en .

:plant_growth_form_hummock
    a skos:Concept ;
    skos:broader :trait_0030010 ;
    skos:definition "Plant that grows as a dense mound up to 1 metre in height. The term generally applies only to species within the genera Triodia that form such mounds and these are considered a distinctive and uniquely Australian growth form."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "hummock"@en .

:plant_growth_form_lycophyte
    a skos:Concept ;
    skos:broader :trait_0030010 ;
    skos:definition "A taxonomic grouping that includes quillworts, clubmosses, and spike mosses."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "lycophyte"@en .

:plant_growth_form_mallee
    a skos:Concept ;
    skos:broader :trait_0030010 ;
    skos:definition "Refers to Eucalyptus species which are small trees that form a large lignotuber. They have multiple stems arising at or near ground level and regenerate from the lignotuber following fire."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "mallee"@en .

:plant_growth_form_palmoid
    a skos:Concept ;
    skos:broader :trait_0030010 ;
    skos:definition "Plant that bears a rosette-like canopy of typically large, often compound leaves atop a usually thick (pachycaulous), columnar, unbranched or little-branched stem. Palms, tree ferns, trunked cycads, trunked grass-trees, and Pandanus are mapped to this term. (Synonyms, rosette-tree, palm, grass-tree)"@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "palmoid"@en .

:plant_growth_form_shrub
    a skos:Concept ;
    skos:broader :trait_0030010 ;
    skos:definition "A woody plant that is distinguished by NOT being a tree, due to one of more of the following characteristics, 1) they are less than 8 metres in height; 2) if a plant community includes trees, the shrubs are shorter than the surrounding trees and not part of the canopy; 3) in comparison to trees, the leaves are more distributed along the entire trunk; 4) the plant has multiple relatively narrow stems arising at or near ground level. The definition of `shrub` is complex, as there are many single-stemmed shrubs within Australia and many taxa that are described in the taxonomic literature as a shrub or small tree."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "shrub"@en .

:plant_growth_form_subshrub
