    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "spreading"@en .

:stem_growth_habit_stoloniferous
    a skos:Concept ;
    skos:broader :trait_0030023 ;
    skos:definition "Plants with long horizontal branches either slightly belowground or aboveground, that root at their nodes or tips, producing new plants. (Synonyms, sarmentose)"@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "stoloniferous"@en .

:stem_growth_habit_submerged
    a skos:Concept ;
    skos:broader :trait_0030023 ;
    skos:definition "Plant where most of the leaves are submerged beneath the surface of a waterbody."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "submerged"@en .

:stem_growth_habit_tufted
