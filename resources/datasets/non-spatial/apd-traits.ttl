:bud_bank_location_apical_buds
    a skos:Concept ;
    skos:broader :trait_0030513 ;
    skos:definition "Plants resprout from above-ground apical meristems. Also termed a caudex. Term refers to palm-like plants with aerial buds that resprout, including grass trees, palms, tree ferns, and pandanus. (Synonym, aerial apical sprouter)"@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "apical_buds"@en .

:bud_bank_location_basal_buds
    a skos:Concept ;
    skos:broader :trait_0030513 ;
    skos:definition "Plants resprout from its base, with buds originating from stem tissue (e.g. lignotuber) and root tissue (root crown) not distinguished between. Most buds designated as being basal are likely to be resprouts from a lignotuber, but this trait value is used if the dataset reported `basal buds` rather than `basal stem buds` or `lignotuber`."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "basal_buds"@en .

:bud_bank_location_basal_stem_buds
    a skos:Concept ;
    skos:broader :trait_0030513 ;
    skos:definition "Plants resprout from the base of the stem. Australian plants with basal stem buds usually have a lignotuber, documented in the trait `storage_organ`. (Synonym, rootstock, lignotuberous resprouter)"@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "basal_stem_buds"@en .

:bud_bank_location_bud-bearing_root
    a skos:Concept ;
    skos:broader :trait_0030513 ;
    skos:definition "Root buds produce vertical suckers (from root crown) or adventitious roots. (Synonyms, root-suckering resprouter, adventitious buds)"@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "bud-bearing_root"@en .

:bud_bank_location_epicormic_buds
    a skos:Concept ;
    skos:broader :trait_0030513 ;
    skos:definition "Plants resprout from the stem or trunk."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "epicormic_buds"@en .

:bud_bank_location_fleshy_underground_organ
    a skos:Concept ;
    skos:broader :trait_0030513 ;
    skos:definition "Plants resprouts from buds on a fleshy underground organ that is a taproot tuber, bulb, corm, stem tuber, root tuber, or belowground caudex. (Synonym, geophyte)"@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "fleshy_underground_organ"@en .

:bud_bank_location_none
    a skos:Concept ;
    skos:broader :trait_0030513 ;
    skos:definition "Plants lack regenerative buds."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "none"@en .

:bud_bank_location_rhizome
    a skos:Concept ;
    skos:broader :trait_0030513 ;
    skos:definition "Plants resprout from buds on a rhizome, where it is not specified if the rhizome is woody or non-woody."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "rhizome"@en .

:bud_bank_location_rhizome_non_woody
    a skos:Concept ;
    skos:broader :trait_0030513 ;
    skos:definition "Plants resprout from buds on a fleshy rhizome, rhizophore, or fibrous rhizome."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "rhizome_non_woody"@en .

:bud_bank_location_rhizome_woody
    a skos:Concept ;
    skos:broader :trait_0030513 ;
    skos:definition "Plants resprout from buds on a woody rhizome."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "rhizome_woody"@en .

:bud_bank_location_root_crown
    a skos:Concept ;
    skos:broader :trait_0030513 ;
    skos:definition "Plants resprout from the root crown, the root tissue at the root-stem transition."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "root_crown"@en .

:bud_bank_location_stem_buds
    a skos:Concept ;
    skos:broader :trait_0030513 ;
    skos:definition "Plants resprout from stem buds; this trait value is separate to epicormic buds. (Synonyms, aerial axillary resprouter)"@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "stem_buds"@en .

:bud_bank_location_stem_tuber
    a skos:Concept ;
    skos:broader :trait_0030513 ;
    skos:definition "Plants resprout from buds on a stem tuber."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "stem_tuber"@en .

:bud_bank_location_stolon
    a skos:Concept ;
    skos:broader :trait_0030513 ;
    skos:definition "Horizontal branches (stems) resprout from the base of the plant."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "stolon"@en .

:bud_bank_location_underground_buds
    a skos:Concept ;
    skos:broader :trait_0030513 ;
    skos:definition "Plants that resprout from underground organ, but the type of organ isn't specified."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "underground_buds"@en .

:clonal_spread_mechanism_aboveground_clonal
    a skos:Concept ;
    skos:broader :trait_0030511 ;
    skos:definition "Vegetative buds or plant fragments that can disperse and produce new plants (including axillary buds, bulbils and turions)."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "aboveground_clonal"@en .

:clonal_spread_mechanism_adventitious_roots
    a skos:Concept ;
    skos:broader :trait_0030511 ;
    skos:definition "Adventitious roots can form at nodes on the main roots or aboveground from nodes on stems."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "adventitious_roots"@en .

:clonal_spread_mechanism_belowground_clonal
    a skos:Concept ;
    skos:broader :trait_0030511 ;
    skos:definition "Plants use an unspecified underground organ for clonal spread."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "belowground_clonal"@en .

:clonal_spread_mechanism_bulb
    a skos:Concept ;
    skos:broader :trait_0030511 ;
    skos:definition "Below ground storage organ derived from stem (and sometimes leaf) tissue, that divides into new individuals."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "bulb"@en .

:clonal_spread_mechanism_corm
    a skos:Concept ;
    skos:broader :trait_0030511 ;
    skos:definition "Below ground organ derived from stem tissue that can function either for vegetative spread or to regenerate following a disturbance."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "corm"@en .

:clonal_spread_mechanism_non_clonal
    a skos:Concept ;
    skos:broader :trait_0030511 ;
    skos:definition "Plant does not display clonal growth."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "non_clonal"@en .

:clonal_spread_mechanism_rhizome
    a skos:Concept ;
    skos:broader :trait_0030511 ;
    skos:definition "Plant has an underground stem that has stem buds and forms adventitious roots at nodes; this stem may be woody or non-woody. Rhizomes are both a mechanism for vegetative spread and a strategy to survive fire."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "rhizome"@en .

:clonal_spread_mechanism_root_buds
    a skos:Concept ;
    skos:broader :trait_0030511 ;
    skos:definition "Root buds that produce vertical suckers."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "root_buds"@en .

:clonal_spread_mechanism_root_tuber
    a skos:Concept ;
    skos:broader :trait_0030511 ;
    skos:definition "A swollen root that functions as a storage organ and has buds that allow clonal growth."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "root_tuber"@en .

:clonal_spread_mechanism_stem_tuber
    a skos:Concept ;
    skos:broader :trait_0030511 ;
    skos:definition "A swollen underground stem that functions as a storage organ and has buds that allow clonal growth."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "stem_tuber"@en .

:clonal_spread_mechanism_stolon
    a skos:Concept ;
    skos:broader :trait_0030511 ;
    skos:definition "Plants having horizontal branches (stems) from the base of the plant that produce new plants from buds at its tip or nodes."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "stolon"@en .

:clonal_spread_mechanism_viviparous
    a skos:Concept ;
    skos:broader :trait_0030511 ;
    skos:definition "Plant produces clonal offspring that grow while still attached to the parent plant."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "viviparous"@en .

:competitive_stratum_ground
    a skos:Concept ;
    skos:broader :trait_0030026 ;
    skos:definition "Graminoids, herbs and ferns reaching maximum heights of c. 0.2-1 m."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "ground"@en .

:competitive_stratum_mid
    a skos:Concept ;
    skos:broader :trait_0030026 ;
    skos:definition "Shrubs reaching maximum heights of c. 1-2 m."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "mid"@en .

:competitive_stratum_post_fire_ephemeral
    a skos:Concept ;
    skos:broader :trait_0030026 ;
    skos:definition "Taxa that avoid competition with other strata by rapidly completing their life cycle after fire; a sub-category of 'ground'."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "post_fire_ephemeral"@en .

:competitive_stratum_upper
    a skos:Concept ;
    skos:broader :trait_0030026 ;
    skos:definition "Tall shrubs reaching maximum heights of c. 3-5 m."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "upper"@en .

:cotyledon_function_foliaceous
    a skos:Concept ;
    skos:broader :trait_0010111 ;
    skos:definition "Cotyledons are photosynthetic and function as leaves."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "foliaceous"@en .

:cotyledon_function_reserve
    a skos:Concept ;
    skos:broader :trait_0010111 ;
    skos:definition "Cotyledons function as energy reserve organs."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "reserve"@en .

:cotyledon_hairs_glabrous
    a skos:Concept ;
    skos:broader :trait_0010113 ;
    skos:definition "Cotyledons lack hairs."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "glabrous"@en .

:cotyledon_hairs_hairy
    a skos:Concept ;
    skos:broader :trait_0010113 ;
    skos:definition "Cotyledons have hairs."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "hairy"@en .

:cotyledon_position_cryptocotylar
    a skos:Concept ;
    skos:broader :trait_0010112 ;
    skos:definition "A type of seed germination in which the cotyledons remain within the seed coat at germination."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "cryptocotylar"@en .

:cotyledon_position_phanerocotylar
    a skos:Concept ;
    skos:broader :trait_0010112 ;
    skos:definition "A type of seed germination in which the cotyledons emerge from the seed coat."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "phanerocotylar"@en .

:diaspore_fleshiness_dry
    a skos:Concept ;
    skos:broader :trait_0012622 ;
    skos:definition "Diaspores where the true fruit, accessory tissues and dispersal appendages are all dry when mature."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "dry"@en .

:diaspore_fleshiness_fleshy
    a skos:Concept ;
    skos:broader :trait_0012622 ;
    skos:definition "Diaspores where either the true fruit, accessory tissues or dispersal appendages are fleshy when mature."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "fleshy"@en .

:dispersal_appendage_aril
    a skos:Concept ;
    skos:broader :trait_0012623 ;
    skos:definition "Fleshy outgrowth of a seed, that often attracts animals like birds or ants."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "aril"@en .

:dispersal_appendage_bracts_and_glumes
    a skos:Concept ;
    skos:broader :trait_0012623 ;
    skos:definition "When the bracts below the inflorescence are persistent and functional as dispersal appendages. This includes persistent glumes, the modified membranous bracts in the family Poaceae that surround the spikelet of a grass."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "bracts_and_glumes"@en .

:dispersal_appendage_bristles
    a skos:Concept ;
    skos:broader :trait_0012623 ;
    skos:definition "Bristle-like projections. Trait value includes awns, which are slender, bristle-like projections in some grasses."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "bristles"@en .

:dispersal_appendage_elaiosome
    a skos:Concept ;
    skos:broader :trait_0012623 ;
    skos:definition "Fleshy (often fatty) appendage on seeds that attracts ants."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "elaiosome"@en .

:dispersal_appendage_fleshy_reward
    a skos:Concept ;
    skos:broader :trait_0012623 ;
    skos:definition "Broad term that includes a suite of appendages that provides a fleshy reward to attract dispersers; highest level of resolution available for some studies."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "fleshy_reward"@en .

:dispersal_appendage_floating_seed
    a skos:Concept ;
    skos:broader :trait_0012623 ;
    skos:definition "Seed that is dispersed by floating on water."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "floating_seed"@en .

:dispersal_appendage_floral_parts
    a skos:Concept ;
    skos:broader :trait_0012623 ;
    skos:definition "When some plant floral parts (the petals, sepals, or style) are persistent and aid in seed and fruit dispersal. This is a broad term not explicitly suggesting a dispersal mechanism."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "floral_parts"@en .

:dispersal_appendage_flotation_scales
    a skos:Concept ;
    skos:broader :trait_0012623 ;
    skos:definition "Scales enhancing flotation, thereby assisting with dispersal."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "flotation_scales"@en .

:dispersal_appendage_hairs
    a skos:Concept ;
    skos:broader :trait_0012623 ;
    skos:definition "Modified hairs that aid in seed dispersal."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "hairs"@en .

