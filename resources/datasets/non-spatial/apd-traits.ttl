    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "bulb"@en .

:storage_organ_caudex
    a skos:Concept ;
    skos:broader :trait_0030512 ;
    skos:definition "Swollen stem that usually functions in water storage, although the term can also apply to below ground structures or to swollen roots."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "caudex"@en .

:storage_organ_corm
    a skos:Concept ;
    skos:broader :trait_0030512 ;
    skos:definition "Below ground storage organ that is a short, rounded, vertical, fleshy, underground stem."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "corm"@en .

:storage_organ_fleshy_underground_organ
    a skos:Concept ;
    skos:broader :trait_0030512 ;
