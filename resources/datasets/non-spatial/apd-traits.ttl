    skos:definition "A 2-dimensional shape that is awl-shaped, approximately narrowly oblong to narrowly triangular with a broad base. (Synonyms, awl-shaped)"@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "subulate"@en .

:leaf_shape_terete
    a skos:Concept ;
    skos:broader :trait_0011311 ;
    skos:definition "A 3-dimensional shape that is a long, narrow cylinder approximately circular in cross-section. Terete leaves often taper toward the tip. (Synonym, cylindrical)"@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "terete"@en .

:leaf_shape_triangular
    a skos:Concept ;
    skos:broader :trait_0011311 ;
    skos:definition "A 2-dimensional shape that is triangular to wedge-shaped, with a length:width ratio 2:1 to 3:2 and the petiole attaching near the widest portion of the leaf."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "triangular"@en .

:leaf_shape_trullate
    a skos:Concept ;
    skos:broader :trait_0011311 ;
    skos:definition "A 2-dimensional shape that is approximately parallelogram-shaped, widest near where the petiole attaches (base), and with a length:width ratio 2:1 to 3:2."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "trullate"@en .

:leaf_shape_widely_elliptical
    a skos:Concept ;
    skos:broader :trait_0011311 ;
    skos:definition "A 2-dimensional shape that is a broad oval, with curved margins, lacking a distinct point, and a length:width ratio of approximately 6:5."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "widely_elliptical"@en .

:leaf_shape_widely_obovate
    a skos:Concept ;
    skos:broader :trait_0011311 ;
    skos:definition "A 2-dimensional shape that is broadly egg-shaped with a length:width ratio of approximately 6:5. The leaf lamina has curved outer margins and narrows nearly to a point where it attaches to the petiole."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "widely_obovate"@en .

:leaf_shape_widely_obtrullate
    a skos:Concept ;
    skos:broader :trait_0011311 ;
    skos:definition "A 2-dimensional shape that is broadly parallelogram-shaped, widest near the tip, and with a length:width ratio of approximately 6:5."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "widely_obtrullate"@en .

:leaf_shape_widely_ovate
    a skos:Concept ;
    skos:broader :trait_0011311 ;
    skos:definition "A 2-dimensional shape that is broadly egg-shaped with a length:width ratio of approximately 6:5. The petiole attaches near the wide, curved portion of the leaf lamina."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "widely_ovate"@en .

:leaf_stomatal_distribution_amphistomatic
    a skos:Concept ;
    skos:broader :trait_0011613 ;
    skos:definition "Leaves with stomata on both leaf surfaces."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "amphistomatic"@en .

:leaf_stomatal_distribution_amphistomatic_uneven
    a skos:Concept ;
    skos:broader :trait_0011613 ;
    skos:definition "Leaves with stomata on both leaf surfaces but more stomata on the lower leaf surface."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "amphistomatic_uneven"@en .

:leaf_stomatal_distribution_epistomatic
    a skos:Concept ;
    skos:broader :trait_0011613 ;
    skos:definition "Leaves that only have stomata on the upper (adaxial) leaf surface."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "epistomatic"@en .

:leaf_stomatal_distribution_hypostomatic
    a skos:Concept ;
    skos:broader :trait_0011613 ;
    skos:definition "Leaves that only have stomata on the lower (abaxial) leaf surface."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "hypostomatic"@en .

:leaf_stomatal_hairs_stomata_covered_with_dense_hairs
    a skos:Concept ;
    skos:broader :trait_0011614 ;
    skos:definition "Stomata densely covered by hairs."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "stomata_covered_with_dense_hairs"@en .

:leaf_stomatal_hairs_stomata_not_covered_by_hairs
    a skos:Concept ;
    skos:broader :trait_0011614 ;
    skos:definition "Stomata not hair-covered."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "stomata_not_covered_by_hairs"@en .

:leaf_type_broadleaf
    a skos:Concept ;
    skos:broader :trait_0011310 ;
    skos:definition "Flat leaf lamina, regardless of whether the leaves are or are not toothed, lobed, or compound."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "broadleaf"@en .

:leaf_type_leafless
    a skos:Concept ;
    skos:broader :trait_0011310 ;
    skos:definition "An adult stage plant that lacks leaves throughout the year."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "leafless"@en .

:leaf_type_needle
    a skos:Concept ;
    skos:broader :trait_0011310 ;
    skos:definition "Needle or awl-shaped leaf that is generally 3-dimensional."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "needle"@en .

:leaf_type_scale
    a skos:Concept ;
    skos:broader :trait_0011310 ;
    skos:definition "Leaf reduced to a small scale."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "scale"@en .

:life_form_aerophyte
    a skos:Concept ;
    skos:broader :trait_0030011 ;
    skos:definition "Plant that obtains moisture and nutrients from the air and rain, resting on other plants or the ground surface."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "aerophyte"@en .

:life_form_chamaephyte
    a skos:Concept ;
    skos:broader :trait_0030011 ;
    skos:definition "Plant whose perennating buds are on persistent shoots that do not extend more than 50 cm above the soil surface. These are normally woody perennials."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "chamaephyte"@en .

:life_form_cryptophyte
    a skos:Concept ;
    skos:broader :trait_0030011 ;
    skos:definition "Plant whose perennating buds are subterranean or under water. This grouping can be further divided into geophyte, helophyte, and hydrophyte."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "cryptophyte"@en .

:life_form_epiphyte
    a skos:Concept ;
    skos:broader :trait_0030011 ;
    skos:definition "Plant that grows on the surface of another plant."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "epiphyte"@en .

:life_form_geophyte
    a skos:Concept ;
    skos:broader :trait_0030011 ;
    skos:definition "Plant that perennates as an organ (rhizome, bulb, corm, or tuber) buried in dry soil, with all leaves dying back each year; a subcategory of cryptophyte."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "geophyte"@en .

:life_form_helophyte
    a skos:Concept ;
    skos:broader :trait_0030011 ;
    skos:definition "Plant that perennates as an organ (rhizome, bulb, corm, or tuber) buried in marshy or wet soil or lying under water, with buds emerging during the growing season and shoots and leaves rising about the water surface; a subcategory of cryptophyte. (Synonyms, emergent_hydrophyte)"@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "helophyte"@en .

:life_form_hemicryptophyte
    a skos:Concept ;
    skos:broader :trait_0030011 ;
    skos:definition "Plant whose perennating buds are at or near the soil surface."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "hemicryptophyte"@en .

:life_form_hydrophyte
    a skos:Concept ;
    skos:broader :trait_0030011 ;
    skos:definition "Plant that perennates as buds or as an organ (rhizome, bulb, corm, or tuber) buried in marshy or wet soil or lying under water, with shoots emerging during the growing season that remain below or at the water surface; a subcategory of cryptophyte."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "hydrophyte"@en .

:life_form_phanerophyte
    a skos:Concept ;
    skos:broader :trait_0030011 ;
    skos:definition "Plant whose perennating buds are on persistent shoots which rise at least 50 cm above the soil surface. These are normally woody perennials."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "phanerophyte"@en .

:life_form_therophyte
    a skos:Concept ;
    skos:broader :trait_0030011 ;
    skos:definition "Annual plants whose shoot and root systems die completely at the end of flowering and seed production, such that they survive the unfavourable cold or dry season in the form of seeds."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "therophyte"@en .

:life_history_annual
    a skos:Concept ;
    skos:broader :trait_0030012 ;
    skos:definition "A plant that lives for up to one year after germinating, completing its life cycle in a single growing season."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "annual"@en .

:life_history_biennial
    a skos:Concept ;
    skos:broader :trait_0030012 ;
    skos:definition "A plant that lives for up to two years after germinating, requiring two growing seasons to complete its life cycle and dying following the second growing season."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "biennial"@en .

:life_history_ephemeral
    a skos:Concept ;
    skos:broader :trait_0030012 ;
    skos:definition "A very short-lived plant, generally with a lifespan of only a few months and the exact length of lifespan quite variable and determined by environmental conditions."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "ephemeral"@en .

