:serotiny_not_serotinous
    a skos:Concept ;
    skos:broader :trait_0030412 ;
    skos:definition "Plant does not display serotiny."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "not_serotinous"@en .

:serotiny_serotinous
    a skos:Concept ;
    skos:broader :trait_0030412 ;
    skos:definition "Plant serotinous, level not specified."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "serotinous"@en .

:serotiny_serotiny_high
    a skos:Concept ;
    skos:broader :trait_0030412 ;
    skos:definition "Plant displays a strong level of serotiny, indicating all seeds are retained as a canopy seedbank until an environmental cue triggers their release."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "serotiny_high"@en .

:serotiny_serotiny_low
    a skos:Concept ;
    skos:broader :trait_0030412 ;
    skos:definition "Plant displays a weak level of serotiny, indicating it releases most of its seeds gradually, with only a small proportion of the seeds retained as a canopy seedbank until an environmental cue triggers the release of the entire seedbank."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "serotiny_low"@en .

:serotiny_serotiny_moderate
    a skos:Concept ;
    skos:broader :trait_0030412 ;
    skos:definition "Plant displays a moderate level of serotiny, indicating it releases some of its seeds gradually, with the remainder retained as a canopy seedbank until an environmental cue triggers the release of the entire seedbank."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "serotiny_moderate"@en .

:sex_type_androdioecious
    a skos:Concept ;
    skos:broader :trait_0030060 ;
    skos:definition "Taxa in which hermaphroditic and staminate (male) plants coexist within a population."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "androdioecious"@en .

:sex_type_andromonoecious
    a skos:Concept ;
    skos:broader :trait_0030060 ;
    skos:definition "Taxa with hermaphrodite flowers and staminate flowers (producing just male gametes) on the same plant but no pistillate (female) flowers."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "andromonoecious"@en .

:sex_type_dioecious
    a skos:Concept ;
    skos:broader :trait_0030060 ;
    skos:definition "Taxa that produce male and female flowers on separate plants."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "dioecious"@en .

:sex_type_gynodioecious
    a skos:Concept ;
    skos:broader :trait_0030060 ;
    skos:definition "Taxa in which hermaphroditic and pistillate (female) plants coexist within a population."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "gynodioecious"@en .

:sex_type_gynomonoecious
    a skos:Concept ;
    skos:broader :trait_0030060 ;
    skos:definition "Taxa with hermaphrodite flowers and pistillate flowers (producing just female gametes) on the same plant but no staminate (male) flowers."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "gynomonoecious"@en .

:sex_type_hermaphrodite
    a skos:Concept ;
    skos:broader :trait_0030060 ;
    skos:definition "Taxa with flowers that produce both male and female gametes; bisexual."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "hermaphrodite"@en .

:sex_type_monoecious
    a skos:Concept ;
    skos:broader :trait_0030060 ;
    skos:definition "Taxa that have both male and female flowers on the same plant but an individual flower produces only male or female gametes."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "monoecious"@en .

:sex_type_polygamodioecious
    a skos:Concept ;
    skos:broader :trait_0030060 ;
    skos:definition "Taxa where some flowers on a plant are hermaphrodite and other are pistillate (female) or staminate (male)."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "polygamodioecious"@en .

:sex_type_polygamonoecious
    a skos:Concept ;
    skos:broader :trait_0030060 ;
    skos:definition "Taxa where some plants in a population have hermaphrodite and pistillate (female) flowers and others have hermaphrodite and staminate (male) flowers."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "polygamonoecious"@en .

:sex_type_polygamous
    a skos:Concept ;
    skos:broader :trait_0030060 ;
    skos:definition "Taxa with both unisexual and bisexual flowers on the same plant."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "polygamous"@en .

:sex_type_trioecious
    a skos:Concept ;
    skos:broader :trait_0030060 ;
    skos:definition "Taxa where plants in a population have either hermaphroditic (bisexual) flowers, pistillate (female) flowers, or staminate (male) flowers."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "trioecious"@en .

:sex_type_unknown
    a skos:Concept ;
    skos:broader :trait_0030060 ;
    skos:definition "Inadequate information to assign a sex type."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "unknown"@en .

:stem_growth_habit_arborescent
