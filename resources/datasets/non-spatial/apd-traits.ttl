    skos:broader :trait_0030411 ;
    skos:definition "Seeds stored in the soil."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "soil_seedbank"@en .

:seedbank_location_soil_seedbank_absent
    a skos:Concept ;
    skos:broader :trait_0030411 ;
    skos:definition "Seed explicitly not stored in the soil."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "soil_seedbank_absent"@en .

:seedbank_longevity_class_long_persistent
    a skos:Concept ;
    skos:broader :trait_0030413 ;
    skos:definition "Seed that persists in the seedbank for more than 5 years, although the time interval varies by study."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "long_persistent"@en .

:seedbank_longevity_class_persistent
    a skos:Concept ;
    skos:broader :trait_0030413 ;
    skos:definition "Seed that survives in the seedbank until at least its second germination season."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "persistent"@en .

:seedbank_longevity_class_short_persistent
    a skos:Concept ;
    skos:broader :trait_0030413 ;
    skos:definition "Seed that persists in the seedbank for approximately 1 to 5 years, although the time interval varies by study."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "short_persistent"@en .

:seedbank_longevity_class_transient
    a skos:Concept ;
    skos:broader :trait_0030413 ;
    skos:definition "Seed that survives in the seedbank until its first annual germination season, but not until its second annual germination season. Some transient seeds will germinate within weeks to months, while others may remain in the seedbank for slightly over a year, depending on how the annual recruitment season aligns with seed maturation. Transient seeds are non-dormant, remaining in the seedbank until environmental conditions are appropriate for germination."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "transient"@en .

:seedbank_longevity_class_widely_dispersed
    a skos:Concept ;
    skos:broader :trait_0030413 ;
    skos:definition "Seed longevity not important, as seeds are widely dispersed and always available for germination."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "widely_dispersed"@en .

:seedling_establishment_conditions_establish_anytime
    a skos:Concept ;
    skos:broader :trait_0030217 ;
    skos:definition "Seedlings can establish immediately following a disturbance and in later years as vegetation ages, right thru to mature and over-mature vegetation"@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "establish_anytime"@en .

:seedling_establishment_conditions_establish_intermediate_to_mature_vegetation
    a skos:Concept ;
    skos:broader :trait_0030217 ;
    skos:definition "Seedlings require some environmental characteristics not found in vegetation straight after fires to establish, for they cannot establish within the first season or two after fire but can establish in older vegetation (including mature to over-mature vegetation)."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "establish_intermediate_to_mature_vegetation"@en .

:seedling_establishment_conditions_establish_post_disturbance
    a skos:Concept ;
    skos:broader :trait_0030217 ;
    skos:definition "Seedling establishment occurs following disturbances that increase light levels at the ground surface, but the type of disturbance is not documented."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "establish_post_disturbance"@en .

:seedling_establishment_conditions_establish_post_fire
    a skos:Concept ;
    skos:broader :trait_0030217 ;
    skos:definition "Seedling establishment occurs following fire."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "establish_post_fire"@en .

