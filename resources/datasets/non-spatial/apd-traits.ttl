    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "bushy"@en .

:stem_growth_habit_caespitose
    a skos:Concept ;
    skos:broader :trait_0030023 ;
    skos:definition "Multiple short-shoots arise from the base of the plant forming a dense tuft, as in a bunch-grass. (Synonyms, cespitose, bunch-grass, tussock)"@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "caespitose"@en .

:stem_growth_habit_climbing
    a skos:Concept ;
    skos:broader :trait_0030023 ;
    skos:definition "Stem that does not have a self-supporting structure and ascends by leaning on or attaching itself to surrounding vegetation or another substrate. Climbing mechanisms include tendrils, petioles, adventitious roots, scrambling, and twining and are distinguished between in the trait `plant_climbing_mechanism`."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "climbing"@en .

:stem_growth_habit_compact
    a skos:Concept ;
    skos:broader :trait_0030023 ;
    skos:definition "Plant with closely packed branches."@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "compact"@en .

:stem_growth_habit_creeping
    a skos:Concept ;
    skos:broader :trait_0030023 ;
    skos:definition "Stem that creeps along the ground, rooting at nodes. (Synonyms, repent)"@en ;
    skos:inScheme <https://w3id.org/APD/traits> ;
    skos:prefLabel "creeping"@en .

:stem_growth_habit_cushion-forming
    a skos:Concept ;
    skos:broader :trait_0030023 ;
