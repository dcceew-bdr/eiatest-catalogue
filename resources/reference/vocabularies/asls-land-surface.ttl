  rdfs:isDefinedBy asls:land-surface ;
  skos:notation "N" ;
  skos:note "Vertical interval usually < 0.3 m and horizontal interval 3 - 10m"@en ;
  skos:prefLabel "Normal gilgai"@en ;
  skos:topConceptOf asls:land-surface ;
.
