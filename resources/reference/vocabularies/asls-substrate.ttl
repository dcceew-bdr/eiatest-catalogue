  skos:prefLabel "Creep deposit"@en ;
.
subst:genesis-CN
  a subst:Mass-genetic-type ;
  a subst:Rock-genesis-hardened-artificial ;
  a skos:Concept ;
  rdfs:isDefinedBy asls:substrate ;
  skos:inScheme asls:substrate ;
  skos:topConceptOf asls:substrate ;
  skos:definition "artificial conglomerate rock mass of selected size grade material that has been hardened using Portland cement or other kinds of cement. Concrete is a weak rock (ca. 35 MPa unconfined compressive strength) usually reinforced with steel to increase its tensile strength." ;
  skos:notation "CN" ;
  skos:prefLabel "Concrete"@en ;
.
subst:genesis-CO
  a subst:Mass-genetic-type ;
  a subst:Rock-genesis-sediments ;
  a skos:Concept ;
  rdfs:isDefinedBy asls:substrate ;
  skos:inScheme asls:substrate ;
  skos:topConceptOf asls:substrate ;
  skos:definition "sediment mass deposited from transport down a slope by gravity (scree), landslide (landslide deposit), mudflow (mudflow deposit), creep (creep deposit) or sheet flow (sheet flow deposit), but not by stream flow. Compared with alluvium, colluvium lacks bedding structure; is more variable in grain size (that is, more poorly sorted); contains much local material; and is generally much more angular. Coarse particles may have particular alignments." ;
  skos:notation "CO" ;
  skos:prefLabel "Colluvium"@en ;
.
subst:genesis-DR
  a subst:Mass-genetic-type ;
  a subst:Rock-genesis-weathered ;
  a skos:Concept ;
  rdfs:isDefinedBy asls:substrate ;
  skos:inScheme asls:substrate ;
  skos:topConceptOf asls:substrate ;
  skos:definition "weathered material (typically soft and clay-rich) produced by thorough decomposition of rock masses due to exposure to land surface processes, but with no transport. It generally lacks any structures that may have been present in the unweathered rock (see also saprolite and partially weathered rock)." ;
  skos:notation "DR" ;
  skos:prefLabel "Decomposed rock"@en ;
.
subst:genesis-ED
  a subst:Mass-genetic-type ;
  a subst:Rock-genesis-sediments ;
  a skos:Concept ;
  rdfs:isDefinedBy asls:substrate ;
  skos:inScheme asls:substrate ;
  skos:topConceptOf asls:substrate ;
  skos:definition "sediment mass deposited from transport by the wind." ;
  skos:notation "ED" ;
  skos:prefLabel "Eolian sediment"@en ;
.
subst:genesis-ES
  a subst:Mass-genetic-type ;
  a subst:Rock-genesis-sediments ;
  a skos:Concept ;
  rdfs:isDefinedBy asls:substrate ;
  skos:inScheme asls:substrate ;
  skos:topConceptOf asls:substrate ;
  skos:definition "eolian sediment of sand size, often taking the form of dunes, with characteristic bedding structures." ;
  skos:notation "ES" ;
  skos:prefLabel "Eolian sand"@en ;
.
subst:genesis-ET
  a subst:Mass-genetic-type ;
  a subst:Rock-genesis-unweathered ;
  a skos:Concept ;
  rdfs:isDefinedBy asls:substrate ;
  skos:inScheme asls:substrate ;
  skos:topConceptOf asls:substrate ;
  skos:definition "consolidated sedimentary rock consisting of clastic material deposited by the wind.* Includes bioclastic calcarenites." ;
  skos:notation "ET" ;
  skos:prefLabel "Eolianite"@en ;
.
subst:genesis-EV
  a subst:Mass-genetic-type ;
  a subst:Rock-genesis-hardened-evaporite ;
  a skos:Concept ;
  rdfs:isDefinedBy asls:substrate ;
  skos:inScheme asls:substrate ;
  skos:topConceptOf asls:substrate ;
  skos:definition "weak sedimentary rock or sediment formed by the precipitation of solutes from water bodies on the land surface, typically as lacustrine sediments. Includes halite and gypsum." ;
  skos:notation "EV" ;
  skos:prefLabel "Evaporite"@en ;
.
subst:genesis-FC
  a subst:Mass-genetic-type ;
  a subst:Rock-genesis-hardened-chemical ;
  a skos:Concept ;
  rdfs:isDefinedBy asls:substrate ;
  skos:inScheme asls:substrate ;
  skos:topConceptOf asls:substrate ;
  skos:definition "indurated material rich in hydrated oxides of iron (usually goethite and hematite) occurring as cemented nodules and/or concretions, or as massive sheets. This material has been commonly referred to in local usage around Australia as laterite, duricrust or ironstone." ;
  skos:notation "FC" ;
  skos:prefLabel "Ferricrete"@en ;
.
subst:genesis-FI
  a subst:Mass-genetic-type ;
  a subst:Rock-genesis-sediments ;
  a skos:Concept ;
  rdfs:isDefinedBy asls:substrate ;
  skos:inScheme asls:substrate ;
  skos:topConceptOf asls:substrate ;
  skos:definition "mass of artificial sediment formed by earth-moving works. Fill is sometimes compacted to the status of a very weak rock mass (stabilised soil), but typically remains an earth mass (Table 36). Garbage forms a very low-density, low-strength fill." ;
  skos:notation "FI" ;
  skos:prefLabel "Fill"@en ;
.
subst:genesis-GY
  a subst:Mass-genetic-type ;
  a subst:Rock-genesis-hardened-evaporite ;
  a subst:Rock-genesis-sediments ;
  a skos:Concept ;
  rdfs:isDefinedBy asls:substrate ;
  skos:inScheme asls:substrate ;
  skos:topConceptOf asls:substrate ;
  skos:definition "evaporite consisting of hydrated calcium sulphate. Non-hydrated calcium sulphate forms closely related masses called anhydrite. It may subsequently be transported by wind as fine crystals and form lunettes or more widespread sedimentary layers blanketing the landscape." ;
  skos:notation "GY" ;
  skos:prefLabel "Gypsum"@en ;
.
subst:genesis-HA
  a subst:Mass-genetic-type ;
  a subst:Rock-genesis-hardened-evaporite ;
  a skos:Concept ;
  rdfs:isDefinedBy asls:substrate ;
  skos:inScheme asls:substrate ;
  skos:topConceptOf asls:substrate ;
  skos:definition "evaporite consisting of sodium chloride." ;
  skos:notation "HA" ;
  skos:prefLabel "Halite (rock salt)"@en ;
.
subst:genesis-IG
  a subst:Mass-genetic-type ;
  a subst:Rock-genesis-unweathered ;
  a skos:Concept ;
  rdfs:isDefinedBy asls:substrate ;
  skos:inScheme asls:substrate ;
  skos:topConceptOf asls:substrate ;
  skos:definition "strong or very strong rock masses formed by solidification of molten rock matter (magma) derived from below the Earth’s surface. The rocks are mainly composed of interlocking crystals. Types are distinguished in Table 34. Plutonic rocks and volcanic rocks are included." ;
  skos:notation "IG" ;
  skos:prefLabel "Igneous rocks"@en ;
.
subst:genesis-IN
  a subst:Mass-genetic-type ;
  a subst:Rock-genesis-unweathered ;
  a skos:Concept ;
  rdfs:isDefinedBy asls:substrate ;
  skos:inScheme asls:substrate ;
  skos:topConceptOf asls:substrate ;
  skos:definition "very weak to strong volcanic rock mass deposited from a flow of ash, the stronger forms being welded together by residual heat during deposition." ;
  skos:notation "IN" ;
  skos:prefLabel "Ignimbrite"@en ;
.
subst:genesis-KC
  a subst:Mass-genetic-type ;
  a subst:Rock-genesis-hardened-chemical ;
  a skos:Concept ;
  rdfs:isDefinedBy asls:substrate ;
  skos:inScheme asls:substrate ;
  skos:topConceptOf asls:substrate ;
  skos:definition "any cemented, terrestrial carbonate accumulation that may vary significantly in morphology and degree of cementation. Also known as carbonate pan, calcareous pan, caliche, kunkar, secondary limestone, travertine. All show slight to strong effervescence with 1-molar HCl." ;
  skos:notation "KC" ;
  skos:prefLabel "Calcrete"@en ;
.
subst:genesis-LA
  a subst:Mass-genetic-type ;
  a subst:Rock-genesis-sediments ;
  a skos:Concept ;
  rdfs:isDefinedBy asls:substrate ;
  skos:inScheme asls:substrate ;
  skos:topConceptOf asls:substrate ;
  skos:definition "sediment mass deposited from transport by waves and from sediment solution and suspension in still water in a closed depression on land." ;
  skos:notation "LA" ;
  skos:prefLabel "Lacustrine sediment"@en ;
.
subst:genesis-LC
  a subst:Mass-genetic-type ;
  a subst:Rock-genesis-hardened-chemical ;
  a skos:Concept ;
  rdfs:isDefinedBy asls:substrate ;
  skos:inScheme asls:substrate ;
  skos:topConceptOf asls:substrate ;
  skos:definition "strongly indurated siliceous material cemented by, and largely composed of, forms of silica, including quartz, chalcedony, opal and chert." ;
  skos:notation "LC" ;
  skos:prefLabel "Silcrete"@en ;
.
subst:genesis-LD
  a subst:Mass-genetic-type ;
  a subst:Rock-genesis-sediments ;
  a skos:Concept ;
  rdfs:isDefinedBy asls:substrate ;
  skos:inScheme asls:substrate ;
  skos:topConceptOf asls:substrate ;
  skos:definition "colluvium rapidly displaced many metres downslope by failure of a mass of earth or rock. If the mass is not already a part of the regolith, the landslide incorporates it in the regolith. Original rock structures are fragmented and disorganised by the action of the landslide." ;
  skos:notation "LD" ;
  skos:prefLabel "Landslide deposit"@en ;
.
subst:genesis-LO
  a subst:Mass-genetic-type ;
  a subst:Rock-genesis-sediments ;
  a skos:Concept ;
  rdfs:isDefinedBy asls:substrate ;
  skos:inScheme asls:substrate ;
  skos:topConceptOf asls:substrate ;
  skos:definition "eolian sediment of silt size." ;
  skos:notation "LO" ;
  skos:prefLabel "Loess"@en ;
.
subst:genesis-MA
  a subst:Mass-genetic-type ;
  a subst:Rock-genesis-sediments ;
  a skos:Concept ;
  rdfs:isDefinedBy asls:substrate ;
  skos:inScheme asls:substrate ;
  skos:topConceptOf asls:substrate ;
  skos:definition "sediment mass deposited from transport by waves and from solution and suspension in sea water." ;
  skos:notation "MA" ;
  skos:prefLabel "Marine sediment"@en ;
.
subst:genesis-MD
  a subst:Mass-genetic-type ;
  a subst:Rock-genesis-sediments ;
  a skos:Concept ;
  rdfs:isDefinedBy asls:substrate ;
  skos:inScheme asls:substrate ;
  skos:topConceptOf asls:substrate ;
  skos:definition "colluvium mixed with water to form dense fluid, and rapidly displaced metres or kilometres downslope. The material is more thoroughly disaggregated than that of a landslide deposit and lacks the bedding and sorting of grain sizes seen in alluvium." ;
  skos:notation "MD" ;
  skos:prefLabel "Mudflow deposit"@en ;
.
subst:genesis-ME
  a subst:Mass-genetic-type ;
  a subst:Rock-genesis-unweathered ;
  a skos:Concept ;
  rdfs:isDefinedBy asls:substrate ;
  skos:inScheme asls:substrate ;
  skos:topConceptOf asls:substrate ;
  skos:definition "moderately strong to very strong rock masses formed by the chemical and physical alterations of igneous or sedimentary rocks under high temperatures and/or very high pressures within the Earth’s crust. Types are distinguished in Table 34." ;
  skos:notation "ME" ;
  skos:prefLabel "Metamorphic rocks"@en ;
.
subst:genesis-PA
  a subst:Mass-genetic-type ;
  a subst:Rock-genesis-sediments ;
  a skos:Concept ;
  rdfs:isDefinedBy asls:substrate ;
  skos:inScheme asls:substrate ;
  skos:topConceptOf asls:substrate ;
  skos:definition "fine-grained calcareous eolian sediment consisting of 30–70% clay." ;
  skos:notation "PA" ;
  skos:prefLabel "Parna"@en ;
.
subst:genesis-PC
